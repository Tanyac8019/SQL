Create a pet store database

CREATE TABLE store (id integer primary key, name TEXT, quantity INTEGER, price INTEGER);
INSERT INTO store VALUES (1, "dogs", 10, 20);
INSERT INTO store VALUES (2, "cats", 15, 18);
