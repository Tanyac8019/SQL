INSERT INTO store VALUES (13, "dragons", 5, 17);
INSERT INTO store VALUES (14, "chinchillas", 6, 25);
INSERT INTO store VALUES (15, "tortoise", 7, 21);
SELECT * FROM store 

Display the database ordered by price
ORDER BY price asc; 

What is the total numer of animals in the store
SELECT * FROM store WHERE quantity >=10;
SELECT SUM(quantity) FROM store; 
